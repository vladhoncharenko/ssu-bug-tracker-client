<!DOCTYPE html>
<html ng-app="ssuBugTracker">
<head>
    <meta charset="utf-8" name="viewport" content="initial-scale=1.0, user-scalable=no">
    <title>Bug <%= bugId %></title>
    <script src="../scripts/libs/angular.js"></script>
    <script src="../scripts/app.js" type="text/javascript"></script>
    <script src="../scripts/services/data-context.js" type="text/javascript"></script>
    <script src="../scripts/controllers/single-bug-controller.js" type="text/javascript"></script>
</head>
<body ng-init="init('<%= bugId %>')" ng-controller="SingleBugController">
<div>
    <p>{{singleBugData.bugId}}</p>
    <p>{{singleBugData.author.firstName}}</p>
    <p>{{singleBugData.author.lastName}}</p>
    <p>{{singleBugData.date}}</p>
    <p>{{singleBugData.location.building}}</p>
    <p>{{singleBugData.location.room}}</p>
    <p>{{singleBugData.picId}}</p>
    <p>{{singleBugData.status}}</p>
</div>
</body>
</html>